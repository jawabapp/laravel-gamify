{
  "name": "jawabapp/laravel-gamify",
  "description": "Add gamification in laravel app with reputation point and badges support",
  "homepage": "https://github.com/jawabapp/laravel-gamify",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "jawabapp",
    "laravel",
    "achivement",
    "gamify",
    "gamification",
    "reputation",
    "points",
    "badge",
    "reward"
  ],
  "authors": [
    {
      "name": "Ahmed Magdy",
      "email": "trmdy@hotmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.33",
    "laravel/framework": "~5.4.0|~5.5.0|~5.6.0|~5.7.0|~5.8.0|^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.8|^4.0",
    "mockery/mockery": "^0.9.4 || ~1.0",
    "phpunit/phpunit": "~8.5"
  },
  "autoload": {
    "psr-4": {
      "Jawabapp\\Gamify\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Jawabapp\\Gamify\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Jawabapp\\Gamify\\GamifyServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
